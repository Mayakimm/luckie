<div class="d-flex align-items-center" id="profile-container">
  <div class="profile-img-container">
    <p class="profile-img mb-0">img</p>
  </div>
  <div class="profile-info d-flex flex-column justify-content-center">
    <%= link_to edit_user_path(current_user), class:"text-decoration-none text-dark" do %>
      <i class="fa-solid fa-gear setting-icon"></i>
    <% end %>
    <p class="mb-0 fs-2"><%= current_user.nickname %></p>
    <p class="mb-0">Following 0  Followers 0</p>
  </div>
</div>
<div class="d-flex" id="dashboard-container">
  <div class="dashboard d-flex flex-column justify-content-between">
    <div class="plan-container d-flex justify-content-between">
      <div class="plans-btn"> Weekly Plans </div>
      <div class="plans-btn"> Monthly Plans </div>
    </div>
    <div class="weekly-dashboard">
      <div class="weekly-graph-container">
        <h6>graph</h6>
      </div>
    </div>
  </div>
  <div class="dashboard">
    <div class="monthly-dashbord">
      <%= month_calendar do |date| %>
        <%= date %>
      <% end %>
    </div>
  </div>

</div>
